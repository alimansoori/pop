    "cheerio": "^1.0.0-rc.12",
    "config": "^3.3.8",
    "convert-csv-to-array": "^1.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-graphql": "^0.12.0",
    "forever": "^4.0.3",
    "google-spreadsheet": "^3.2.0",
    "graphql": "^16.7.1",
    "http-errors": "~1.6.3",
    "mariadb": "^3.0.1",
    "mongodb": "^5.3.0",
    "mongoose": "^7.3.1",
    "node-dev": "^7.4.3",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "puppeteer": "^20.7.1",
    "puppeteer-extra": "^3.3.6",
