{
  "name": "pup",
  "version": "1.0.0",
  "description": "Automated software",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets.ts",
    "lint": "eslint src --fix",
    "tsc": "tsc.cmd",
    "build22": "npm-run-all clean lint tsc copy-assets",
    "build": "npm-run-all lint tsc",
    "prestart": "npm run build",
    "dev:start": "npm-run-all build start",
    "dev": "cross-env NODE_ENV=development nodemon --watch src -e ts,js --exec npm run dev:start",
    "production": "cross-env NODE_ENV=production nodemon --watch src -e ts,js --exec npm run dev:start",
    "start": "node .",
    "server": "nodemon ./dist/server.js",
    "rest-server": "nodemon ./dist/rest-server.js",
    "test": "cross-env NODE_ENV=test jest --watch",
    "jest": "jest",
    "watch": "tsc.cmd --watchAll",
    "client": "npx http-server -c-1 -p 8080 -P http://localhost:3000",
    "search": "node dist/searchWithinSheet.js",
    "import": "node dist/importFromSheet.js",
    "update-amazons": "node dist/script/updateAmazonsByKeepa.js",
    "update-sources": "node dist/script/updateSources.js",
    "google-search": "node dist/searchInGoogle.js",
    "delete": "node dist/deleteBads.js"
  },
  "author": "Ali",
  "license": "ISC",
